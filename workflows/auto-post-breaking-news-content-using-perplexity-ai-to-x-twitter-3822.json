      {
        "id": "d549c019-1f3e-4758-a5ee-e4ac7e03cc2d",
        "name": "searchQuery",
        "type": "n8n-nodes-base.set",
        "position": [
          -340,
          0
        ],
        "parameters": {
          "options": {},
          "assignments": {
            "assignments": [
              {
                "id": "4cfbc312-5dcd-444d-ae08-0bab417c084c",
                "name": "searchInput",
                "type": "string",
                "value": "What's the latest news in artificial intelligence?"
